{
    "name": "leinonen/yii2-monolog",
    "type": "yii2-extension",
    "description": "Monolog extension for Yii2",
    "license": "MIT",
    "authors": [
        {
            "name": "Juuso Leinonen",
            "email": "leinonen.juuso@gmail.com"
        }
    ],
    "require": {
        "monolog/monolog": "^1.22",
        "yiisoft/yii2": "~2.0.11",
        "php": "^7.0",
        "mockery/mockery": "0.9.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "leinonen\\Yii2Monolog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "leinonen\\Yii2Monolog\\Tests\\": "tests/"
        }
    }
}
